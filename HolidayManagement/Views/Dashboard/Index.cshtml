@model HolidayManagement.Models.DashboardViewModel
@{
    ViewBag.Title = "Index";
}

<h2>@Model.Message</h2>

<div>
<ul class="nav nav-pills">
    <li class="active"><a data-toggle="pill" href="#users">Users</a></li>
    <li><a data-toggle="pill" href="#mycalendar">My Calendar</a></li>
    <li><a data-toggle="pill" href="#groupmanagement">Group Management</a></li>
    <li><a data-toggle="pill" href="#settings">Settings</a></li>

</ul>
</div>
<div class="tab-content">
    <div id="users" class="tab-pane fade in active">
       @Html.Partial("_Users")
    </div>
    <div id="mycalendar" class="tab-pane fade">
        @Html.Partial("_MyCalendar")
    </div>
    <div id="groupmanagement" class="tab-pane fade">
      @Html.Partial("_GroupMAnagement")
    </div>
    <div id="settings" class="tab-pane fade">
       @Html.Partial("_Settings")
    </div>
</div>

<script src="~/Scripts/Dashboard/UserModel.js"></script>
<script src="~/Scripts/Dashboard/TeamModel.js"></script>
<script type="text/javascript" src="~/Scripts/Dashboard/DashboardModel.js" ></script>
<script type="text/javascript">
    InitializeDashboardModel(@Html.Raw(Json.Encode(Model)));
</script>